    <link rel="canonical" href="http://www.example.com/">
    -->

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">

   <!-- <link rel="stylesheet" href="$$hosted_libs_prefix$$/$$version$$/material.cyan-light_blue.min.css">-->
    <link rel="stylesheet" href="css/styles.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  </head>
  <body style="background-image: url(image/blockchain.jpg);">
    <div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">

      <div class="android-header mdl-layout__header mdl-layout__header--waterfall" style="background-color: transparent;">
        <div class="mdl-layout__header-row">
          <div class="android-title mdl-layout-title">
            <h4 style="font-family: 'Orbitron', sans-serif;"><b>CRYPTEX 2.0</b></h4>
          </div>
          <!-- Add spacer, to align navigation to the right in desktop -->


          <!-- <span class="android-mobile-title mdl-layout-title" >
             <h5 style="font-family: 'Orbitron', sans-serif; " ><b>CRYPTEX 2.0</b></h5>
          </span> -->

        </div>
      </div>

      <div class="android-drawer mdl-layout__drawer">
        <span class="mdl-layout-title" style="align-items: center;">
        <img  src="image/user.jpg" alt="user" style="border-radius: 50%; margin-top: 10px;">
        </span>
        <p style="color:white"></p>
        <nav class="mdl-navigation">
          <a class="mdl-navigation__link" href="/levels">Levels</a>
          <a class="mdl-navigation__link" href="/leaderboard">Leaderboard</a>
          <a class="mdl-navigation__link" href="/kozhi">Update Access</a>
          <a class="mdl-navigation__link" href="">About</a>
          <a class="mdl-navigation__link" href="">Legacy</a>
          <a class="mdl-navigation__link" href="/logout">Logout</a>
          <!--<div class="android-drawer-separator"></div> -->

        </nav>
      </div>
   <div class="android-content mdl-layout__content">


<ul class="demo-list-item mdl-list">
  <% for(var i=0;i<doc.length;i++){ %>
    <% if(doc[i].user_access === 0) %>
  <li class="mdl-list__item">
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].auth.name %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].auth.id %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].auth.email %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].college %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].unlocked %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].timestamp %>
    </span>
    <span class="mdl-list__item-primary-content" style="color:white;">
      <%= doc[i].user_access %>
    </span>
  </li>
  <% } %>

</ul>
    </div>

    <script src="$$hosted_libs_prefix$$/$$version$$/material.min.js"></script>
  </body>
</html>
